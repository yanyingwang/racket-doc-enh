{
  "name": "Doc Racket Enhancement",
  "author": "Yanying Wang",
  "version": "0.1",
  "homepage_url": "https://github.com/yanyingwang/doc-racket-enh",
  "description": "a web browser plugin to enhance Racket Doc",
  "page_action": {
    "browser_style": true,
    "default_title": "Doc Racket Enhancement",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "show_matches": [ "https://docs.racket-lang.org/*",
                      "file:///Applications/Racket%20v*/*",
                      "file:///Users/*/*/docs/*"
                    ]
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      // "css": ["style.css"],
      "js": [ "jquery-3.5.1.min.js", "content_script.js" ],
      "matches": [ "https://docs.racket-lang.org/*",
                   "file:///Applications/Racket%20v*/*",
                   "file:///Users/*/*/doc/*"
                    ]
    }
  ],
  "manifest_version": 2
}

